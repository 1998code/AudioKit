<% parameters.each do |sp_parameter| -%>
<% sp_parameter.each do  |sp_var, data| -%>
    /// <%= data["comment"] %>
    public var <%= data["ak-variable"] %>: Double = <%= data["default"] %> {
        didSet {
            internalAU?.<%= data["ak-variable"] %> = Float(<%= data["ak-variable"] %>)
        }
    }

    /// Ramp to <%= data["ak-variable"] %> over 20 ms
    ///
    /// - parameter <%= data["ak-variable"] %>: Target <%= data["comment"] %>
    ///
    public func ramp(<%= data["ak-variable"] %> <%= data["ak-variable"] %>: Double) {
        <%= data["ak-variable"] %>Parameter?.setValue(Float(<%= data["ak-variable"] %>), originator: token!)
    }

<% end -%>
<% end -%>
{
    "name": "AudioKit",
    "version": "3.7.1",
    "authors": {
        "Aurelius Prochazka": "audiokit@audiokit.io"
    },
    "license": {
        "type": "MIT",
        "file": "LICENSE"
    },
    "homepage": "http://audiokit.io/",
    "social_media_url": "http://twitter.com/AudioKitMan",
    "documentation_url": "http://audiokit.io/docs/",
    "source": {
        "http": "https://github.com/audiokit/AudioKit/releases/download/v3.7.1/AudioKit.framework.zip"
    },
    "summary": "Open-source audio synthesis, processing, & analysis platform.",
    "platforms": {
        "osx": "10.11",
        "ios": "9.0",
        "tvos": "9.0"
    },
    "frameworks": [
	"AVFoundation"
    ],
    "pod_target_xcconfig": {
	"ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "YES"
    },
    "osx": {
        "source_files": [
		"macOS/AudioKit.framework/Versions/A/Headers/*.h",
		"macOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "public_header_files": [
		"macOS/AudioKit.framework/Versions/A/Headers/*.h",
		"macOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "vendored_frameworks": [
		"macOS/AudioKit.framework",
		"macOS/AudioKitUI.framework"
	]
    },
    "ios": {
       "source_files": [
		"iOS/AudioKit.framework/Versions/A/Headers/*.h",
		"iOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "public_header_files": [
		"iOS/AudioKit.framework/Versions/A/Headers/*.h",
		"iOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "vendored_frameworks": [
		"iOS/AudioKit.framework",
		"iOS/AudioKitUI.framework"
	]
    },
    "tvos": {
        "source_files": [
		"tvOS/AudioKit.framework/Versions/A/Headers/*.h",
		"tvOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "public_header_files": [
		"tvOS/AudioKit.framework/Versions/A/Headers/*.h",
		"tvOS/AudioKitUI.framework/Versions/A/Headers/*.h"
	],
        "vendored_frameworks": [
		"tvOS/AudioKit.framework",
		"tvOS/AudioKitUI.framework"
	]
    }
}
